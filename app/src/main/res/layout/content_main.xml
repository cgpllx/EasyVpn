<?xml version="1.0" encoding="utf-8"?>
<cc.easyandroid.easyrecyclerview.core.progress.EasyProgressRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/qFFFFFF"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <CheckBox
        android:id="@+id/on_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:button="@drawable/on_off_selector"
        android:gravity="center"
        android:text="@null" />

</cc.easyandroid.easyrecyclerview.core.progress.EasyProgressRelativeLayout>
